<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container 2.4//EN"
	"http://www.seasar.org/dtd/components24.dtd">
<components>
	<include path="j2ee.dicon"/>

	<component
		class="org.seasar.jms.core.deploy.impl.WmqResourceAdapterDeployer"
	>
		<!-- スレッドプールのスレッド数 -->
		<arg>10</arg>

		<!-- Generic Resource Adapter for JMS へのパス -->
		<property name="path">
			"ra/genericra-1.5.rar"
		</property>

		<!--
			Generic Resource Adapter for JMS のプロパティを設定します．
			設定可能なプロパティは
				https://genericjmsra.dev.java.net/docs/userguide/userguide.html#Resource_Adapter_properties
			の「Resource Adapter properties」を参照してください．
		-->
		<initMethod name="setProperty">
			<arg>"userName"</arg>
			<arg>"koichik"</arg>
		</initMethod>
		<initMethod name="setProperty">
			<arg>"password"</arg>
			<arg>"hi_mi_tsu"</arg>
		</initMethod>
	</component>
</components>
